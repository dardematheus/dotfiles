{
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.iconTheme": "symbols",
    "workbench.startupEditor": "none",
    "workbench.editor.labelFormat": "default",
    "workbench.statusBar.visible": false,
    "workbench.layoutControl.enabled": false,
    "workbench.activityBar.location": "hidden",
    "workbench.preferredDarkColorTheme": "Rosé Pine Moon (no italics)", 
    "workbench.preferredLightColorTheme": "Rosé Pine Dawn (no italics)",

    "editor.mouseWheelZoom": true,
    "editor.fontFamily": "JetBrains Mono Nerd Font",
    "editor.fontWeight": 600,
    "editor.fontSize": 14,
    "editor.lineHeight": 1.4,
    "editor.renderLineHighlight": "gutter",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "editor.fontLigatures": true,
    "editor.cursorStyle": "block",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "hidden",
    "editor.semanticHighlighting.enabled": false,
    "editor.inlineSuggest.suppressSuggestions": true,
    
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.compactFolders": false,
    
    "window.menuBarVisibility": "compact",
    "window.titleBarStyle": "custom",
    "window.commandCenter": false,

    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "JetBrains Nerd Font",
    "telemetry.telemetryLevel": "off",

    "symbols.hidesExplorerArrows": false,
    "breadcrumbs.enabled": false,
    "rust-analyzer.linkedProjects": [],
    "discord.lowerDetailsNoWorkspaceFound": "Something",
    "discord.lowerDetailsEditing": "Something",
    "github.copilot.advanced": {},
    "apc.electron": {
        "titleBarStyle": "hiddenInset",
    },
    "apc.header": {
        "height": 36
    },
    "apc.listRow": {
        "height": 23
    },
    "apc.font.family": "Inter",
    "apc.stylesheet": {
        ".title-label > h2": "display: none",
        ".editor-actions": "display: none",
        ".nosidebar .inline-tabs-placeholder": "width: 75px",
        ".pane-header": "padding: 0 8px",
        ".split-view-view:first-child .pane-header": "display: none !important;",
        ".monaco-list-row": "border-radius: 4px",
        ".monaco-workbench . monaco-list:not(.element.focused):focus:before": "display: none;",
    },
    "errorLens.delay": 600,
    "animations.Install-Method": "Apc Customize UI+++",
    "github.copilot.editor.enableAutoCompletions": true,
    "workbench.colorTheme": "Rosé Pine Dawn (no italics)",
}